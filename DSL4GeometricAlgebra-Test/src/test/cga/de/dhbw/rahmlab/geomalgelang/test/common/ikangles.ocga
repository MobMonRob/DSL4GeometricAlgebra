// finding the joint angles

L01 := (ε₀^ε₃^εᵢ)*
L12 := (P1^P2^εᵢ)*
L23 := (P2^P3^εᵢ)*

P0 := ε₀ //createPoint(0,0,0);

a1 := ε₂
b1 := -PIc
N1 := ε₁^ε₂
x1 := (a1^b1)/N1
y1 := a1⋅b1;

a2 := (L01*⋅ε₀)⋅εᵢ
b2 := (L12*⋅ε₀)⋅εᵢ
N2 := -(PIc*⋅ε₀)⋅εᵢ
x2 := (a2^b2)/N2
