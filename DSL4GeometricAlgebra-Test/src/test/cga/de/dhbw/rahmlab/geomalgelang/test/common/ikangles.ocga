a4 := (L23*⋅ε₀)⋅εᵢ
b4 := (L45*⋅ε₀)⋅εᵢ
N4 := -(PIc*⋅ε₀)⋅εᵢ
x4 := (a4^b4)/N4
y4 := a4⋅b4

a5 := Pc;
b5 := -ae
N5 := (-L45^ε₀)⋅εᵢ
x5 := (a5∧b5)/N5
y5 := a5⋅b5

a6 := (L45*⋅ε₀)⋅εᵢ
b6 := -se
N6 := -ae (ε₃^ε₂^ε₁)
x6 := (a6∧b6)/N6
y6 := a6⋅b6
atan2(y1,x1), atan2(y2,x2), atan2(y3,x3), atan2(y4, x4), atan2(y5, y5), atan2(y6,x6)
